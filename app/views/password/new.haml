.form_container
  - form_tag(:action => "create", :method => "post", :controller => "password") do
    = password_field_tag "new_password"
    = password_field_tag "new_password_confirmation"
    = hidden_field_tag "user_id", @user_id
    = hidden_field_tag "token", @token_value
    = submit_tag "Update Password", :id => "submit", :name => "submit"

    
  